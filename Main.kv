<Inventario>:
  orientation: 'vertical'
  Widget:
    size_hint:(.2, .2)
    Label: 
      text:"PROJECT M.A.E"
      font_size:70 
      center_x: root.width /3
      top: root.top - 10
      
  BoxLayout:
    size_hint:(1, .8)
    
    BoxLayout:
      orientation: 'vertical'
      pos_hint: {'left': 1}
      padding:10
      BoxLayout:
        orientation: 'vertical'
        
        BoxLayout:
          size_hint:(1, .2)
          pos_hint: {"top": 0}
          TextInput:
            id : pesquisa
            font_size:30
            size_hint:(.8, .6)
            focus:True
        BoxLayout:
          size_hint:(1, .1)
          pos_hint: {"top": 0}
          Button:
            text:"Search"
            size_hint:(.8, 1)
            on_press: app.pesquisa()
          Button:
            text:"+"
            size_hint:(.2, 1)
            on_press:app.add()
        BoxLayout:
          size: root.size
          BoxLayout:
            size_hint:(.3, 1)
            orientation: 'vertical'
            Label:
              text: 'Name:'
            Label:
              text: 'Unity:'
            Label:
              text: 'Pos:'
            Label:
              text: 'Type:'
            Label:
              text: 'Buy date:'
            Label:
              text: 'Link:'
            Label:
              text: 'Description:'
          BoxLayout:
            orientation: 'vertical'
            halign:"left"
            TextInput:
              id: name
              multiline:False
              text: 'name'
            TextInput:
              id: unity
              multiline:False
              text: 'unity'
            TextInput:
              id : pos
              multiline:False
              text: 'pos x,y'
            TextInput:
              id: type
              multiline:False
              text: 'type'
            TextInput:
              id: buy_date
              multiline:False
              text: 'buy date DD/MM/YY'
            TextInput:
              id: link
              multiline:True
              text: 'link'
            TextInput:
              id:description
              multiline:True
              text: 'description'
          
      
    BoxLayout:
      GridLayout:
        cols:8
        rows:12
        padding:10

        Button:
          id: 0_0
          on_press: app.teste()
        Button:
          id: 0_1
